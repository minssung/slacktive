{
  "name": "DockerMan",
  "version": "1.0.0",
  "description": "",
  "main": "./server/index.js",
  "private": true,
  "scripts": {
    "start": "node ./server/index.js",
    "debug": "nodemon --watch ./server --inspect=0.0.0.0:9229 --nolazy ./server/index.js",
    "debug-windows": "nodemon --legacy-watch --watch ./server --inspect=0.0.0.0:9229 --nolazy ./server/index.js",
    "client": "npm run start --prefix client",
    "server": "nodemon server/index.js",
    "pm2-dev": "pm2 start ./ecosystem.config.js",
    "pm2-product": "pm2 start ./ecosystem.config.js --env production",
    "nodemon": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" ",
    "dev": "concurrently --kill-others-on-fail  \"npm run pm2-dev\" \"npm run client\" ",
    "product": "concurrently --kill-others-on-fail \"npm run pm2-product\" \"npm run client\" ",
    "root": "npm run client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "agenda": "^3.0.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "immutability-helper": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "node-cron": "^2.0.3",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
