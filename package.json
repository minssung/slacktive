    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.2",
    "express-session": "^1.17.0",
    "immutability-helper": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "react-live-clock": "^3.1.0",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
